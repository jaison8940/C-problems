/* 
input : 
1234
output :
0001
0010
0011
1000
*/
#include<stdio.h>
#include <stdlib.h>
int main()
{
    char str[16];
    scanf("%s",str);
    int i,j,temp;
    for(i=0;i<strlen(str);i++)
    {
        temp = str[i] - '0';
        for(j=8;j>0;j=j/2)
        {
            (temp & j)? printf("1"):printf("0");
        }
        printf("\n");
    }
}
